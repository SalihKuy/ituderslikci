{
  "name": "ituderslikci",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "main.js",
  "scripts": {
    "start": "vite dev",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "dist": "electron-builder",
    "dist:mac-arm": "electron-builder --mac --arm64",
    "dist:win-x86": "electron-builder --win --ia32",
    "dist:all": "electron-builder --mac --arm64 --win --ia32"
  },
  "dependencies": {
    "chrome-finder": "^1.0.7",
    "fs": "^0.0.1-security",
    "node-fetch": "^3.3.2",
    "path": "^0.12.7",
    "puppeteer-core": "^24.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^34.0.2",
    "electron-builder": "^24.0.0",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "vite": "^6.0.5",
    "vite-plugin-electron": "^0.29.0",
    "wait-on": "^8.0.2"
  },
  "build": {
    "appId": "com.salihkuy.ituderslikci",
    "productName": "ituderslikci",
    "directories": {
      "output": "dist",
      "buildResources": "build"
    },
    "compression": "maximum",
    "asar": true,
    "asarUnpack": [
      "**/node_modules/puppeteer-core/**/*"
    ],
    "files": [
      "dist-electron/**/*",
      "dist/renderer/**/*",
      "main.js",
      "preload.js",
      "src/puppeteer.js",
      "src/token.js",
      "package.json"
    ],
    "extraResources": [],
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "arm64"
          ]
        }
      ],
      "category": "public.app-category.utilities",
      "icon": "build/icon.icns",
      "hardenedRuntime": false,
      "gatekeeperAssess": false,
      "identity": null
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32"
          ]
        }
      ],
      "icon": "build/icon.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "build"
    }
  }
}